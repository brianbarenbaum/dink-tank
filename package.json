{
	"name": "codex-vue-starter",
	"private": true,
	"version": "0.1.0",
	"description": "Starter template for Vite + Vue 3 + Tailwind 4 + shadcn-vue projects using a Codex agent-engineering workflow.",
	"type": "module",
	"scripts": {
		"setup": "bash scripts/setup.sh",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"worker:dev": "wrangler dev --config worker/wrangler.toml",
		"worker:deploy:dry": "wrangler deploy --config worker/wrangler.toml --dry-run",
		"format": "biome format --write .",
		"format:check": "biome check .",
		"lint:check": "biome check .",
		"typecheck": "vue-tsc --noEmit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"review:dispatch": "bash scripts/review-dispatch.sh",
		"review:base": "bash scripts/review-run-all.sh --mode base",
		"review:uncommitted": "bash scripts/review-run-all.sh --mode uncommitted",
		"review:full": "bash scripts/review-run-all.sh --mode full",
		"ingest:help": "node --experimental-strip-types data-ingestion/run.ts --help",
		"ingest:crossclub": "node --experimental-strip-types data-ingestion/run.ts bootstrap all",
		"ingest:crossclub:weekly": "node --experimental-strip-types data-ingestion/run.ts weekly all",
		"ingest:crossclub:players": "node --experimental-strip-types data-ingestion/run.ts bootstrap players",
		"ingest:crossclub:standings": "node --experimental-strip-types data-ingestion/run.ts bootstrap standings",
		"ingest:crossclub:teams": "node --experimental-strip-types data-ingestion/run.ts bootstrap teams",
		"ingest:crossclub:matchups": "node --experimental-strip-types data-ingestion/run.ts bootstrap matchups",
		"ingest:crossclub:playoffs": "node --experimental-strip-types data-ingestion/run.ts bootstrap playoff-matchups",
		"ingest:crossclub:details": "node --experimental-strip-types data-ingestion/run.ts bootstrap details",
		"ingest:crossclub:details:weekly": "node --experimental-strip-types data-ingestion/run.ts weekly details",
		"ingest:crossclub:dry-run": "node --experimental-strip-types data-ingestion/run.ts bootstrap all --dry-run",
		"ingest:crossclub:details:strict": "node --experimental-strip-types data-ingestion/run.ts bootstrap details --strict-dependency-guard",
		"ingest:crossclub:details:weekly:strict": "node --experimental-strip-types data-ingestion/run.ts weekly details --strict-dependency-guard",
		"ingest:sqlite:init": "node --experimental-strip-types data-ingestion/sqlite/init.ts",
		"ingest:sqlite:mock": "node --experimental-strip-types data-ingestion/sqlite/run-mock.ts bootstrap all",
		"ingest:sqlite:verify": "node --experimental-strip-types data-ingestion/sqlite/verify.ts"
	},
	"dependencies": {
		"@langchain/classic": "^1.0.17",
		"@langchain/core": "^1.1.24",
		"@langchain/openai": "^1.2.7",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"langchain": "^1.2.24",
		"lucide-vue-next": "^0.564.0",
		"pg": "^8.18.0",
		"pinia": "^3.0.3",
		"tailwind-merge": "^3.4.0",
		"typeorm": "^0.3.28",
		"vue": "^3.5.18",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.15",
		"@cloudflare/workers-types": "^4.20260204.0",
		"@playwright/test": "^1.55.0",
		"@tailwindcss/vite": "^4.1.12",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^25.2.3",
		"@types/pg": "^8.16.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitest/coverage-v8": "^4.0.18",
		"@vue/test-utils": "^2.4.6",
		"better-sqlite3": "^12.6.2",
		"jsdom": "^28.0.0",
		"shadcn-vue": "^2.4.3",
		"tailwindcss": "^4.1.12",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.3",
		"vitest": "^4.0.8",
		"vue-tsc": "^3.1.0",
		"wrangler": "^4.40.2"
	}
}
